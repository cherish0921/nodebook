                <legend>用户登录</legend>
                <div class="form-item">
                    <label for="user">用户名</label>
                    <span></span>
                </div>
                <div class="form-item">
                    <label for="psd">密码</label>
                    <span></span>
                </div>
                <div class="form-item">
                    <button type="submit" class="btn">退出登录</button>
                </div>
            </fieldset>
        </form>
    </div>
</body>
</html>